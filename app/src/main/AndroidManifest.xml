<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.smartsaleerp.smartsale"

    >

    <dist:module dist:instant="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity android:name="com.smartsaleerp.smartsale.OrderDetails"
                android:theme="@style/OrderDetailsTheme"
            ></activity>
        <activity android:name="com.smartsaleerp.smartsale.MenuItem" />
        <activity
            android:name="com.smartsaleerp.smartsale.OrderMeal"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.smartsaleerp.smartsale.TopUp"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.smartsaleerp.smartsale.MainMessage"
            android:screenOrientation="portrait" />
        <activity android:name="com.smartsaleerp.smartsale.Messages" />
        <activity
            android:name="com.smartsaleerp.smartsale.smartsale_dashborad"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.smartsaleerp.smartsale.ValidateCredentials"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.smartsaleerp.smartsale.MainActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>